{
  "name": "my-rescript-app",
  "reason": {
    "react-jsx": 3
  },
  "sources": {
    "dir" : "src",
    "subdirs" : true
  },
  "bsc-flags": [
    "-bs-super-errors",
    "-bs-no-version-header"
  ],
  "package-specs": [
    {
      "module": "es6",
      "in-source": true
    }
  ],
  "suffix": ".bs.js",
  "namespace": true,
  "bs-dependencies": [
    "reason-react",
    "bs-css",
    "bs-css-emotion"
  ],
  "ppx-flags": ["@davesnx/styled-ppx/ppx"],
  "bs-dev-dependencies": [
    "@glennsl/bs-jest",
    "bs-react-testing-library"
  ],
  "gentypeconfig": {
    "language": "typescript",
    "shims": {},
    "generatedFileExtension": ".gen.tsx",
    "module": "es6",
    "debug": {
      "all": false,
      "basic": false
    }
  },
  "refmt": 3
}
